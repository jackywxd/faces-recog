{
  "name": "web",
  "version": "0.1.0",
  "description": "人脸识别系统前端应用",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:staging": "NODE_ENV=production NEXT_PUBLIC_ENV=staging next build",
    "build:production": "NODE_ENV=production NEXT_PUBLIC_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "preview": "next build && npx serve@latest out",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npx @cloudflare/next-on-pages && wrangler pages deploy .vercel/output/static",
    "pages:dev": "npx @cloudflare/next-on-pages --dev",
    "deploy:staging": "node scripts/deploy-pages.cjs staging",
    "deploy:production": "node scripts/deploy-pages.cjs production",
    "deploy:preview:staging": "node scripts/deploy-pages.cjs staging --preview",
    "deploy:preview:production": "node scripts/deploy-pages.cjs production --preview",
    "deploy:check:staging": "node scripts/deploy-pages.cjs staging --check",
    "deploy:check:production": "node scripts/deploy-pages.cjs production --check",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@face-recog/database": "workspace:*",
    "@face-recog/shared": "workspace:*",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.300.0",
    "next": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@face-recog/config": "workspace:*",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "wrangler": "^4.25.0"
  }
}
