{
  "name": "@face-recog/api",
  "version": "0.1.0",
  "description": "Face recognition API service using Hono on Cloudflare Workers",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "build": "tsc && echo 'Build completed'",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "preview": "wrangler deploy --dry-run",
    "preview:staging": "wrangler deploy --env staging --dry-run",
    "preview:production": "wrangler deploy --env production --dry-run",
    "logs": "wrangler tail",
    "logs:staging": "wrangler tail --env staging",
    "logs:production": "wrangler tail --env production",
    "setup:r2": "node scripts/setup-r2.cjs",
    "setup:secrets": "node scripts/setup-secrets.cjs",
    "check": "node scripts/deploy-check.cjs",
    "check:staging": "node scripts/deploy-check.cjs staging",
    "check:production": "node scripts/deploy-check.cjs production"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.1",
    "hono": "^3.12.8",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@face-recog/config": "workspace:*",
    "@face-recog/shared": "workspace:*",
    "@types/node": "catalog:",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "wrangler": "^3.22.1"
  },
  "type": "module"
}
